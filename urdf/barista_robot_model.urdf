  <!-- JOINTS -->

    <!-- Base joint-->
  <joint name="bottom_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.0951"/>
    <parent link="base_link"/>
    <child link="bottom_link"/>
  </joint>

  <joint name="spacer_1_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.030 -0.128 0.1475"/>
    <parent link="bottom_link"/>
    <child link="spacer_1_link"/>
  </joint>

  <joint name="spacer_2_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.030 0.128 0.1475"/>
    <parent link="bottom_link"/>
    <child link="spacer_2_link"/>
  </joint>

  <joint name="spacer_3_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.030 -0.128 0.1475"/>
    <parent link="bottom_link"/>
    <child link="spacer_3_link"/>
  </joint>

  <joint name="spacer_4_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.030 0.128 0.1475"/>
    <parent link="bottom_link"/>
    <child link="spacer_4_link"/>
  </joint>

  <!-- Wheel joint -->
  <joint name="left_wheel_joint" type="continuous">
    <origin rpy="0 1.571 1.571" xyz="0 0.158 -0.0599"/>
    <parent link="bottom_link"/>
    <child link="left_wheel_link"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  
  <joint name="right_wheel_joint" type="continuous">
    <origin rpy="0 1.571 1.571" xyz="0 -0.158 -0.0599"/>
    <parent link="bottom_link"/>
    <child link="right_wheel_link"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <!-- Caster wheel joint -->
  <!-- <joint name="caster_wheel_support_joint" type="fixed">
