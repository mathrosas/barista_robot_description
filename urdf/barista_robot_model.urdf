    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="caster_wheel_roll_link"/>
    <child link="caster_wheel_pitch_link"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" velocity="100.0"/>
    <dynamics damping="0.0" friction="0.1"/>
  </joint>

  <!-- Top joint-->
  <joint name="top_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.375"/>
    <parent link="base_link"/>
    <child link="top_link"/>
  </joint>

  <!-- Lidar -->
  <joint name="lidar_support_joint" type="fixed">
    <origin rpy="0 0 3.142" xyz="0.100 0 0.09035"/>
    <parent link="bottom_link"/>
